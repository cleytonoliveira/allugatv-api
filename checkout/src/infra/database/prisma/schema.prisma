generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id          Int      @id @default(autoincrement())
  name        String
  nameId      String   @unique
  productName     String
  productId   String
  price       Float
  expiryDate  DateTime
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
